<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Rectángulo amarillo -->
    <item
        android:left="0dp"
        android:right="0dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="20dp" />
        </shape>
    </item>

    <!-- Rectángulo blanco -->
    <item
        android:left="5dp"
        android:right="485dp"
        android:top="20dp"
        android:bottom="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#92D050" />
            <corners android:radius="20dp" />
        </shape>
    </item>
</layer-list>